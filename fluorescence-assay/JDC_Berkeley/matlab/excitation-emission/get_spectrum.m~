function spectrum = get_spectrum(infile, regexp_string, nrows, ncols)

frewind(infile);

while ~feof(infile)
  line = fgetl(infile);

  % Emission bottom
  S = regexp(line, regexp_string);
  if length(S) > 0
    % Read wavelength line.
    line = fgetl(infile);
    regexp_string = 'Wavelength \d+ \((\d+) nm\)';
    S = regexp(line, regexp_string, 'tokens');
    wavelength = sscanf(char(S{1}(1)), '%d');
    emission_bottom_wavelengths = [emission_bottom_wavelengths wavelength];
  end
  
end
